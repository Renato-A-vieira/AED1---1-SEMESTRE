    "def c_para_f (celsius):\n",
    "    fahrenheit = 9/5 * celsius + 32\n",
    "    return fahrenheit\n",
    "\n",
    "celsius = int(input(\"Digite a temperatura em celsius\"))\n",
    "fahrenheit = c_para_f(celsius)\n",
    "\n",
    "print(f\"{celsius}ºC valem {fahrenheit}ºF\")"
   ]
